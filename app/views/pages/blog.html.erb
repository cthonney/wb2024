<div class="blogs-container">
  <div style="margin-bottom: 30px;">
    <div data-controller="three-test">
      <canvas class="webgl"></canvas>
    </div>
    <canvas class="webgl" data-controller="three"></canvas>
  </div>
  <div class="blogs mx-auto">
    <% @blogs.each do |blog| %>
      <div class='blog card p-4 rounded-4 shadow-2'>
        <h2><%= blog[:title] %></h2>
        <p class='card-text'><%= blog[:content]%> </p>
      </div>
    <% end %>
  </div>
</div>
